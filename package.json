{
  "name": "vlang-playground",
  "version": "1.1.0",
  "description": "Self-contained component to embed in websites for running V code",
  "license": "MIT",
  "keywords": [
    "vlang",
    "runnable code"
  ],
  "main": "dist/vlang-playground.js",
  "author": {
    "name": "V Open Source Community Association",
    "email": "hello@vosca.dev",
    "url": "https://vosca.dev"
  },
  "contributors": [
    {
      "name": "Petr Makhnev",
      "email": "mr.makhneff@gmail.com",
      "url": "https://github.com/i582"
    }
  ],
  "files": [
    "./dist"
  ],
  "scripts": {
    "watch-scss": "sass --watch src/css/index.scss:src/css/index.css",
    "watch": "node esbuild.watch.config.js",
    "prod": "sass src/css/index.scss:src/css/index.css && node esbuild.build.config.js && v run ./bundle-js.v"
  },
  "dependencies": {
    "@codemirror/language": "^6.2.1",
    "esbuild": "^0.16.17",
    "eta": "^1.13.0",
    "uglify-js": "^3.17.4",
    "uglifyjs": "^2.4.11"
  },
  "devDependencies": {
    "@types/codemirror": "^5.60.5",
    "@types/estree": "^1.0.0",
    "@types/tern": "^0.23.4",
    "sass": "^1.62.0",
    "typescript": "~4.9.4"
  }
}
